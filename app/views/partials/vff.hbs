<section class="panel {{type}}" style="height:{{height}}px" data-type="{{type}}" data-dock>
    <div class="progress"></div>

    <video src="/static/media/{{video}}" preload="auto"></video>
    {{#if audio}}
        <audio src="/static/media/{{audio}}" preload="auto" data-subtitles='{{parseSRT subtitles}}' loop></audio>
    {{/if}}

</section>
